{
	"name": "General Tagset",
	"desc": "Converting different tagsets from tree-tagger into general tagset",
	"tagset": {
		"ABB": {
			"desc": "abbreviation",
			"tags": {
				"english": [],
				"russian": [],
				"french": ["ABR"]
			}
		},
		"ADJ": {
			"desc": "adjective, including comparative and superlative (also includes ordinals in English and Russian, not sure about French)",
			"tags": {
				"english": ["JJ", "JJR", "PDT", "JJS"],
				"russian": ["Afc", "Afc---f", "Afp", "Afp*", "Afsfsn", "Mo*", "Afcmsnf"],
				"french": ["ADJ"]
				}
		},
		"ADV": {
			"desc": "adverb",
			"tags": {
